object o {
  var property x = pepita.energia() / 2
}

class C {
  var property x
}

object w inherits C(x = pepita.energia()) { }

object pepita {
  var property energia

  method initialize() {
    energia = 2
  }
}

test "TEST o" {
  assert.equals(1, o.x())
}

test "TEST w" {
  assert.equals(2, w.x())
}